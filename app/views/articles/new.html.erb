<h1>New Article</h1>
<img src="https://images.pexels.com/photos/978587/pexels-photo-978587.jpeg?auto=compress&cs=tinysrgb&dpr=2&h=750&w=1260"/>
<%= form_for :article, url: articles_path do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :text %><br>
    <%= f.text_area :text %>
  </p>
  <p>
    <%= f.submit 'Save Article', id: "save-button" %>
  </p> 
<% end %>
<%= link_to 'Back', articles_path, class: "small button" %>
